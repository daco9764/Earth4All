				<eqn>60</eqn>
				<units>y</units>
			</aux>
			<aux name="tCO2e/tCH4">
				<eqn>23</eqn>
				<units>1</units>
			</aux>
			<aux name="GHG EMISSIONS GtCO2e/y">
				<eqn>&quot;CO2_emissions_GtCO2/y&quot;*&quot;tCO2e/tCO2&quot;+&quot;CH4_emissions_GtCH4/y&quot;*&quot;tCO2e/tCH4&quot;+&quot;N2O_emissions_GtN2O/y&quot;*&quot;tCO2e/tN2O&quot;</eqn>
				<units>GtCO2e/y</units>
			</aux>
			<aux name="tCO2e/tCO2">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<flow name="CH4 emissions GtCH4/y">
				<eqn>&quot;Man-made_CH4_emissions_GtCH4/y&quot;+&quot;Natural_CH4_emissions_GtCH4/y&quot;</eqn>
				<units>Gt/Year</units>
			</flow>
			<flow name="N2O emissions GtN2O/y">
				<eqn>&quot;Natural_N2O_emissions_GtN2O/y&quot;+&quot;Man-made_N2O_emissions_GtN2O/y&quot;</eqn>
				<units>GtN2O/Year</units>
			</flow>
			<aux name="tCO2e/tN2O">
				<doc>	guestimate</doc>
				<eqn>7</eqn>
				<units>1</units>
			</aux>
			<flow name="Extra cooling from ice melt ZJ/y">
				<eqn>&quot;Melting_rate_deep_ice_1/y&quot;*Amount_of_ice_in_1980_Mkm3*Ton_per_m3_ice*&quot;Heat_required_to_melt_ice_kJ/kg&quot;</eqn>
				<units>ZJ/Year</units>
			</flow>
			<aux name="Ton per m3 ice">
				<eqn>0.95</eqn>
				<units>t/m3</units>
			</aux>
			<aux name="Heat required to melt ice kJ/kg">
				<doc>	smeltevarnen er 6 kJ/mol og jeg gjetter at 1 mol er 1+1+16 = 18 g</doc>
				<eqn>333</eqn>
				<units>kJ/kg</units>
			</aux>
			<flow name="N2O breakdown GtN2O/y">
				<eqn>N2O_in_atmosphere_GtN2O/Life_of_N2O_in_atm_y</eqn>
				<units>GtN2O/Year</units>
			</flow>
			<aux name="Life of N2O in atm y">
				<doc>	from ESMICON, was 100</doc>
				<eqn>95</eqn>
				<units>y</units>
			</aux>
			<flow name="Heat to space ZJ/y">
				<eqn>Extra_heat_in_surface_ZJ*&quot;Transfer_rate_for_heat_going_to_space_1/y&quot;</eqn>
				<units>ZJ/Year</units>
			</flow>
			<aux name="Transfer rate for heat going to space 1/y">
				<eqn>(&quot;Transfer_rate_surface-space_in_1980_1/y&quot;*((Observed_warming_deg_C+297)/297))*(&quot;Albedo_(1)&quot;/&quot;Albedo_in_1980_(1)&quot;)</eqn>
				<units>1/y</units>
			</aux>
			<flow name="Heat to deep ocean ZJ/y">
				<eqn>Extra_heat_in_surface_ZJ*&quot;Transfer_rate_for_heat_going_to_abyss_1/y&quot;</eqn>
				<units>ZJ/Year</units>
			</flow>
			<aux name="Transfer rate surface-space in 1980 1/y">
				<doc>	was 0.01, so 0.02 to obtain observed warming
		211103: changed from 0.001 to 0.01, ie equilibration time 100 years</doc>
				<eqn>0.01</eqn>
				<units>1/y</units>
			</aux>
			<aux name="tCO2 per tCH4">
				<doc>	CO2 = 12+16+16=44  CH4 = 12+1+1+1+1 = 16.   44/16=2.75</doc>
				<eqn>2.75</eqn>
				<units>1</units>
			</aux>
			<flow name="CO2 from CH4 GtCO2/y">
				<eqn>&quot;CH4_breakdown_GtCH4/y&quot;*tCO2_per_tCH4</eqn>
				<units>Gt/y</units>
			</flow>
			<flow name="CH4 breakdown GtCH4/y">
				<eqn>CH4_in_atmosphere_GtCH4/Life_of_CH4_in_atm_y</eqn>
				<units>Gt/Year</units>
			</flow>
