	<model name="Demand">
		<variables>
			<aux name="Workers drawdown period y">
				<doc>	Was 2</doc>
				<eqn>10</eqn>
				<units>1</units>
			</aux>
			<aux name="Workers payback period y">
				<eqn>20</eqn>
				<units>y</units>
			</aux>
			<aux name="Consumption demand G$/y">
				<eqn>&quot;Worker_consumption_demand_G$/y&quot;-&quot;Sales_tax_workers_G$/y&quot;+&quot;Owner_consumption_G$/y&quot;-&quot;Sales_tax_owners_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Total purchasing power G$/y" access="output">
				<eqn>&quot;Worker_cash_inflow_G$/y&quot;+&quot;Govmnt_cash_inflow_G$/y&quot;+&quot;Owner_cash_inflow_G$/y&quot;-&quot;Sales_tax_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="OCI in 1980">
				<doc>	was 307</doc>
				<eqn>7081</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="WFI in 1980">
				<doc>	was 1059
		211127 changed from 14000
		211126 changed from 19015</doc>
				<eqn>13000</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Permanent govmnt cash inflow G$/y">
				<eqn>SMTH1(&quot;Govmnt_cash_inflow_G$/y&quot;, Time_to_adjust_budget_y, GCI_in_1980)</eqn>
				<isee:delay_aux/>
				<units>G$/y</units>
			</aux>
			<aux name="Govmnt cash inflow G$/y">
				<eqn>&quot;Govmnt_net_income_G$/y&quot;-&quot;Cash_flow_from_govmnt_to_banks_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Time to adjust budget y">
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="GCI in 1980">
				<doc>	was 276
		211126 changed from 2576</doc>
				<eqn>5400</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Permanent owner cash inflow G$/y">
				<eqn>SMTH1(&quot;Owner_cash_inflow_G$/y&quot;, Time_to_adjust_owner_consumption_y, OCI_in_1980)</eqn>
				<isee:delay_aux/>
				<units>G$/y</units>
			</aux>
			<aux name="Time to adjust owner consumption y">
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="Worker cash inflow G$/y">
				<eqn>&quot;Worker_income_after_tax_G$/y&quot;-&quot;Cash_flow_from_workers_to_banks_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Worker debt burden y">
				<eqn>&quot;Workers_debt_G$&quot;/&quot;Worker_income_after_tax_G$/y&quot;</eqn>
				<units>y</units>
			</aux>
			<stock name="Workers debt G$">
				<eqn>&quot;Workers_debt_in_1980_G$&quot;</eqn>
				<inflow>&quot;Workers_new_debt_G$/y&quot;</inflow>
				<outflow>&quot;Workers_payback_G$/y&quot;</outflow>
				<units>G$</units>
			</stock>
			<aux name="Time to adjust worker consumption y">
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="Worker finance cost as share of income (1)">
				<eqn>&quot;Cash_flow_from_workers_to_banks_G$/y&quot;/&quot;Worker_income_after_tax_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Cash flow from workers to banks G$/y">
				<eqn>&quot;Worker_interest_cost_G$/y&quot;+&quot;Workers_payback_G$/y&quot;-&quot;Workers_new_debt_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Owner consumption G$/y">
				<eqn>&quot;Permanent_owner_cash_inflow_G$/y&quot;*&quot;Owner_consumptin_fraction_(1)&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Owner consumptin fraction (1)">
				<eqn>1-&quot;Owner_savings_fraction_(1)&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Total savings G$/y" access="output">
				<eqn>&quot;Owner_savings_G$/y&quot;+&quot;Worker_savings_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Max workers debt burden y">
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="Max workers debt G$">
