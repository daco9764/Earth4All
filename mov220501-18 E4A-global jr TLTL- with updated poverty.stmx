	<model name="Climate">
		<variables>
			<flow name="CO2 in atmosphere GtCO2 net flow">
				<eqn>&quot;CO2_emissions_GtCO2/y&quot;+&quot;CO2_from_CH4_GtCO2/y&quot;-&quot;CO2_absorption_GtCO2/y&quot;+&quot;CO2_from_CH4_GtCO2/y&quot;</eqn>
				<units>GtC/Year</units>
			</flow>
			<stock name="CO2 in atmosphere GtCO2">
				<eqn>CO2_in_atm_in_1980_GtCO2</eqn>
				<inflow>CO2_in_atmosphere_GtCO2_net_flow</inflow>
				<units>GtC</units>
			</stock>
			<aux name="GtCO2 per ppm">
				<doc>	211102 changed from 1.8 to 7.92 as per ESMICON</doc>
				<eqn>7.9</eqn>
				<units>GtCO2/ppm</units>
			</aux>
			<aux name="N2O conc in 1980 ppm">
				<eqn>N2O_in_atm_in_1980_GtN2O/Mass_of_atmosphere_Zt</eqn>
				<units>ppm</units>
			</aux>
			<aux name="N2O in atm in 1980 GtN2O">
				<doc>	1.052 Gt N2O according to ESMICON,
		But this equals 1.052 Gt / 5E15 = 1E9/5E15=0.2 E-6 = 200 ppb, and should have been 320 ppb!</doc>
				<eqn>1.052</eqn>
				<units>GtN2O</units>
			</aux>
			<aux name="Mass of atmosphere Zt">
				<doc>	Weight of atmosphere is 5 * 10^15 ton. 29 g/mol</doc>
				<eqn>5</eqn>
				<units>Zt</units>
			</aux>
			<aux name="CH4 conc in 1980 ppm">
				<eqn>CH4_in_atm_in_1980_GtCH4/Mass_of_atmosphere_Zt</eqn>
			</aux>
			<aux name="CH4 in atm in 1980 GtCH4">
				<doc>	3 GtC from UG ESMICON multiplied with 12+1+1+1+1 = 16 / 12 = 1.33</doc>
				<eqn>2.5</eqn>
				<units>Gt/y</units>
			</aux>
			<aux name="OWeoLoCO2">
				<eqn>( IF TIME &gt; 2022 THEN 1+&quot;sOWeoLoCO2&gt;0&quot;*(Observed_warming_deg_C/Observed_warming_in_2022_deg_C-1) ELSE 1 )</eqn>
				<units>1</units>
			</aux>
			<aux name="sOWeoLoCO2&gt;0">
				<doc>	was 0</doc>
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="CH4 forcing per ppm W/m2/ppm">
				<doc>	To get CH4 forcing = 0.5 W/m2 in 2015</doc>
				<eqn>TIME</eqn>
				<gf>
					<yscale min="0.82" max="1.1"/>
					<xpts>1980,2000,2020,2100</xpts>
					<ypts>0.82,0.94,1.01,1.1</ypts>
				</gf>
				<units>W/(m2*ppm)</units>
			</aux>
			<aux name="CO2 concentration in 2022 ppm" access="output">
				<doc>	410 i 2018 plus ca 2 ppm/y</doc>
				<eqn>420</eqn>
				<units>ppm</units>
			</aux>
			<aux name="Melting rate deep ice 1/y">
				<eqn>&quot;Melting_rate_surface_1/y&quot;/&quot;Surface_vs_deep_rate_(1)&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Surface vs deep rate (1)">
				<doc>	211103: changed from 10 to 4</doc>
				<eqn>4</eqn>
				<units>1</units>
			</aux>
			<aux name="Risk of extreme heat event (1)">
				<eqn>Observed_warming_deg_C</eqn>
				<gf>
					<yscale min="1" max="40"/>
					<xpts>0,1.2,2,2.9,5.2</xpts>
					<ypts>1,4.8,8.6,14,40</ypts>
				</gf>
				<units>1</units>
			</aux>
			<aux name="Water vapour concentration 1980 g/kg">
				<doc>	Value from ESMICON-pp</doc>
				<eqn>2</eqn>
				<units>g/kg</units>
			</aux>
			<aux name="Water vapor concentration g/kg">
				<eqn>&quot;Water_vapour_concentration_1980_g/kg&quot;*(1+&quot;sOWeoWV&gt;0&quot;*(Observed_warming_deg_C/Warming_in_1980_deg_C-1))</eqn>
				<units>g/kg</units>
			</aux>
			<aux name="sOWeoWV&gt;0">
				<doc>	220221 Chosen to be smaller than CO2 forcing in 1980 and larger in 2020</doc>
				<eqn>0.18</eqn>
				<units>1</units>
			</aux>
			<aux name="Water vapor feedback in 1980 W/m2">
				<doc>	Equals fraction H20 / CO2 in Nature articlw (=0.8) times CO2 forcing in 1980 (= 1.07 W/m2)</doc>
				<eqn>0.9</eqn>
				<units>W/m2</units>
			</aux>
			<flow name="Extra warming from forcing ZJ/y">
				<doc>	1 TW gir 31.5 ZJ/y (1 Z = 1E21)
		since there are 365*24*60*60 = 31.5 million seconds in a year, and 1 J = 1Ws
		1 W/m2 = 16 ZJ/y</doc>
				<eqn>(&quot;Total_man-made_forcing_W/m2&quot;*Global_surface_Mkm2)*31.5/1000</eqn>
				<units>ZJ/Year</units>
			</flow>
			<aux name="Amount of ice in 1980 Mkm3">
				<doc>	16 Mkm2 is Greenland 2.2 and Antarctica 14.2, say 3km thick =&gt; 52 Mkm3.
		30 Mkm2 is land and ocean ice, say 100 m thick =&gt; 3 Mkm3</doc>
				<eqn>55</eqn>
				<units>Mkm3</units>
			</aux>
			<aux name="sWVeoWVF&gt;0">
				<doc>	Taken from scatter diagram ESMICON-pp Humidity effect on H2O Blocking</doc>
				<eqn>3</eqn>
				<units>1</units>
			</aux>
			<aux name="Melting rate surface in 1980 1/y">
				<doc>	was 0.001</doc>
				<eqn>0.0015</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Perception delay y">
				<eqn>5</eqn>
				<units>y</units>
			</aux>
			<aux name="CO2 in atm in 1980 GtCO2">
				<doc>	211103: changed from 600</doc>
				<eqn>2600</eqn>
				<units>GtCO2</units>
			</aux>
			<aux name="kg CH4 per $ of GDP">
				<eqn>0</eqn>
			</aux>
			<flow name="CO2 absorption GtCO2/y">
				<eqn>(CO2_in_atmosphere_GtCO2-CO2_in_atm_in_1850_GtCO2)/Life_of_extra_CO2_in_atm_y</eqn>
				<units>GtCO2/y</units>
			</flow>
			<aux name="Life of CH4 in atm y">
				<doc>	was 11, so 7, so 8</doc>
				<eqn>7.5</eqn>
				<units>y</units>
			</aux>
			<aux name="CO2 concentration in 1980 ppm">
				<eqn>330</eqn>
				<units>ppm</units>
			</aux>
			<aux name="CO2 in atm in 1850 GtCO2">
				<eqn>2200</eqn>
				<units>GtCO2</units>
			</aux>
			<aux name="Life of extra CO2 in atm y">
				<eqn>Life_of_extra_CO2_in_atm_in_1980_y*OWeoLoCO2</eqn>
				<units>y</units>
			</aux>
			<aux name="Life of extra CO2 in atm in 1980 y">
				<doc>	211102: changed from 9 to 60 to match observed history (after I found a mistake)
