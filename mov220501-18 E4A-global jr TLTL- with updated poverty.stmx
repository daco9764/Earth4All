	<model name="Wellbeing_trust_and_tension">
		<variables>
			<aux name="Exogenous introduction period?">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Exogenous introduction period y">
				<eqn>30</eqn>
				<units>y</units>
			</aux>
			<aux name="Reform delay y">
				<eqn>SMTH1(Indicated_reform_delay_y, Time_to_change_reform_delay_y)</eqn>
				<isee:delay_aux/>
				<units>y</units>
			</aux>
			<aux name="Indicated reform delay y">
				<eqn>Normal_reform_delay_y*&quot;Social_trust_effect_on_reform_delay_(1)&quot;*&quot;Social_tension_effect_on_reform_delay_(1)&quot;</eqn>
			</aux>
			<aux name="Time to change reform delay y">
				<doc>	The time it takes for society to shorten decision and implementation delay.</doc>
				<eqn>10</eqn>
				<units>y</units>
			</aux>
			<aux name="sLPeoAWP&gt;0">
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Threshold participation (1)">
				<eqn>0.8</eqn>
				<units>1</units>
			</aux>
			<aux name="Introduction period for policy y" access="output">
				<eqn>( IF &quot;Exogenous_introduction_period?&quot; &gt; 0 THEN Exogenous_introduction_period_y ELSE Reform_delay_y )</eqn>
				<units>y</units>
			</aux>
			<aux name="Average wellbeing from progress (1)">
				<eqn>(1+&quot;sROPeoAW&gt;0&quot;*(&quot;Observed_rate_of_progress_1/y&quot;-&quot;Threshold_progress_rate_1/y&quot;))*&quot;Wellbeing_effect_of_participation_(1)&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="sROPeoAW&gt;0">
				<doc>	was 1, so 10, so 3, so 8, so 6, so 9</doc>
				<eqn>6</eqn>
				<units>1</units>
			</aux>
			<aux name="Observed rate of progress 1/y">
				<eqn>SMTH1(((&quot;AVERAGE_WELLBEING_INDEX_(1)&quot;-&quot;Past_AWI_(1)&quot;)/&quot;AVERAGE_WELLBEING_INDEX_(1)&quot;)/Average_wellbeing_perception_delay_y, Average_wellbeing_perception_delay_y, 0)</eqn>
				<isee:delay_aux/>
			</aux>
			<aux name="Threshold progress rate 1/y">
				<doc>	If there is progress above 2 % per year, there is social clm</doc>
				<eqn>0.02</eqn>
				<units>1</units>
			</aux>
			<aux name="Wellbeing effect of participation (1)">
				<eqn>1+&quot;sLPeoAWP&gt;0&quot;*(&quot;Labour_participation_rate_(1)&quot;/&quot;Threshold_participation_(1)&quot;-1)</eqn>
			</aux>
			<aux name="AVERAGE WELLBEING INDEX (1)">
				<eqn>(0.5*&quot;Average_wellbeing_from_disposable_income_(1)&quot;+0.5*&quot;Average_wellbeing_from_public_spending_(1)&quot;)*&quot;Average_wellbeing_from_inequality_(1)&quot;*&quot;Average_wellbeing_from_global_warming_(1)&quot;*&quot;Average_wellbeing_from_progress_(1)&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from disposable income (1)">
				<eqn>EXP(&quot;Diminishing_return_disposable_income_(1)&quot;+LN(&quot;Worker_disposable_income_k$/p/y&quot;/&quot;Threshold_disposable_income_k$/p/y&quot;))</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from public spending (1)">
				<eqn>EXP(&quot;Diminishing_return_public_spending_(1)&quot;+LN(&quot;Public_spending_per_person_k$/p/y&quot;/&quot;Threshold_public_spending_k$/p/y&quot;))</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from inequality (1)">
				<eqn>1+&quot;sIIeoAW&lt;0&quot;*(&quot;Inequality_(1)&quot;/&quot;Threshold_inequality_(1)&quot;-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from global warming (1)">
				<eqn>MAX(&quot;Min_wellbeing_from_global_warming_(1)&quot;, MIN(1, 1+&quot;sGWeoAW&lt;0&quot;*(Perceived_warming_deg_C/Threshold_warming_deg_C-1)))</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal reform delay y">
				<eqn>30</eqn>
				<units>1</units>
			</aux>
			<aux name="Social trust effect on reform delay (1)">
				<eqn>1+&quot;sSTReoRD&lt;0&quot;*(&quot;Social_trust_(1)&quot;/&quot;Social_trust_in_1980_(1)&quot;-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Social tension effect on reform delay (1)">
