	<model name="Energy">
		<variables>
			<flow name="Addition of fossil el capacity GW/y">
				<eqn>MAX(0, &quot;Desired_fossil_el_capacity_change_GW/y&quot;)</eqn>
				<units>GW/Year</units>
			</flow>
			<aux name="CAPEX renewable el $/W">
				<eqn>&quot;CAPEX_renewable_el_in_1980_$/W&quot;*&quot;Cost_index_for_sun_and_wind_capacity_(1)&quot;</eqn>
				<units>$/W</units>
			</aux>
			<aux name="OPEX renewable el $/kWh">
				<eqn>0.001</eqn>
				<units>$/kWh</units>
			</aux>
			<aux name="Fraction fossil plus nuclear electricity (1)">
				<eqn>(&quot;Fossil_electricity_production_TWh/y&quot;+&quot;Nuclear_electricity_production_TWh/y&quot;)/&quot;Electricity_production_TWh/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra increase in demand for electricity from NE TWh/y">
				<eqn>&quot;Extra_reduction_in_demand_for_non-el_fossil_fuel_from_NE_Mtoe/y&quot;*&quot;Extra_use_of_electricity_per_reduced_use_of_non-el_FF_MWh/toe&quot;</eqn>
				<units>TWh/y</units>
			</aux>
			<aux name="Extra use of electricity per reduced use of non-el FF MWh/toe">
				<doc>	1.500 l of gasoline can be replaced with 3500 kWh when an electric car replaces a fossil car (ratio 3.5/1.5=2.3)
		12.000 kWh heat in 1.000 l of heating oil can be replaced with 12.000 kWh heat from a heat pump drawing 3.000 KWh el (ratio 12/3 = 4)
		A small petrol driven generator can generate 3kWh/h when drawing 1 liter of fuel pr hour.Can be replaced with a 2 kW electric engine (ratio 3kWh/1 liter = 3)
		Old lightingen bulbs drawing 50 W can be replaced with LED drawing 8 W - I sort this as energy efficiency</doc>
				<eqn>3</eqn>
			</aux>
			<aux name="CAPEX renewable el in 1980 $/W">
				<doc>	211117: changed from 10</doc>
				<eqn>7</eqn>
				<units>$/W</units>
			</aux>
			<aux name="Cost index for sun and wind capacity (1)">
				<eqn>(1-&quot;Cost_reduction_per_doubling_of_sun_and_wind_capacity_(1)&quot;)^&quot;Number_of_doublings_in_sun_and_wind_capacity_(1)&quot;</eqn>
			</aux>
			<stock name="Accumulated sun and wind capacity from 1980 GW">
				<eqn>Sun_and_wind_capacity_in_1980_GW</eqn>
				<inflow>&quot;Addition_of_sun_and_wind_capacity_GW/y&quot;</inflow>
				<units>Gha</units>
			</stock>
			<aux name="Sun and wind capacity in 1980 GW">
				<doc>	Renewable capacity 300 minus hydro capacity 290</doc>
				<eqn>10</eqn>
				<units>GW</units>
			</aux>
			<aux name="Cost reduction per doubling of sun and wind capacity (1)">
				<eqn>0.2</eqn>
				<units>1</units>
			</aux>
			<aux name="Number of doublings in sun and wind capacity (1)">
				<eqn>LN(2)+LN(Accumulated_sun_and_wind_capacity_from_1980_GW/Sun_and_wind_capacity_in_1980_GW)</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra cost per reduced use of non-el FF $/toe">
				<doc>	It costs an extra 200.000 kr/y in battery and power to save 1.500 liters of fuel (Ratio 15k$/1.5 toe=10)</doc>
				<eqn>10</eqn>
				<units>$/toe</units>
			</aux>
			<aux name="Desired fossil el capacity change GW/y">
				<eqn>(Desired_fossil_el_capacity_GW-Fossil_electricity_capacity_GW)/Fossil_el_cap_construction_time_y+&quot;Discard_of_fossil_el_capacity_GW/y&quot;</eqn>
				<units>GW</units>
			</aux>
			<aux name="Desired renewable el capacity change GW">
				<eqn>Desired_renewable_el_capacity_GW-Renewable_electricity_capacity_GW</eqn>
				<units>GW</units>
			</aux>
			<aux name="Desired renewable el capacity GW">
				<eqn>&quot;Desired_supply_of_renewable_electricity_TWh/y&quot;/&quot;Renewable_capacity_up-time_kh/y&quot;</eqn>
				<units>GW</units>
			</aux>
			<stock name="Renewable electricity capacity GW">
				<eqn>Renewable_el_capacity_in_1980_GW</eqn>
				<inflow>&quot;Addition_of_renewable_el_capacity_GW/y&quot;</inflow>
				<outflow>&quot;Discard_of_renewable_el_capacity_GW/y&quot;</outflow>
				<units>GW</units>
			</stock>
			<aux name="Renewable capacity up-time kh/y">
				<doc>	Full time is 8760 hours per year. 3000 h/y is 34%.</doc>
				<eqn>3</eqn>
				<units>h/y</units>
			</aux>
			<aux name="Desired fossil el capacity GW">
				<eqn>&quot;Demand_for_fossil_electricity_TWh/y&quot;/&quot;8_khours_per_year&quot;</eqn>
				<units>GW</units>
			</aux>
			<stock name="Fossil electricity capacity GW">
				<eqn>Fossil_el_capacity_in_1980_GW</eqn>
				<inflow>&quot;Addition_of_fossil_el_capacity_GW/y&quot;</inflow>
				<outflow>&quot;Discard_of_fossil_el_capacity_GW/y&quot;</outflow>
				<units>GW</units>
			</stock>
			<aux name="Fossil el cap construction time y">
				<eqn>3</eqn>
			</aux>
			<flow name="Discard of fossil el capacity GW/y">
				<eqn>Fossil_electricity_capacity_GW/Life_of_fossil_el_capacity_y</eqn>
				<units>GW/Year</units>
			</flow>
			<aux name="Renewable el construction time y">
				<eqn>3</eqn>
			</aux>
			<flow name="Discard of renewable el capacity GW/y">
				<eqn>Renewable_electricity_capacity_GW/Life_of_renewable_el_capacity_y</eqn>
				<units>GW/Year</units>
			</flow>
			<aux name="Fossil capacity up-time kh/y">
				<doc>	 8760 hours per year</doc>
				<eqn>&quot;Demand_for_fossil_electricity_TWh/y&quot;/Fossil_electricity_capacity_GW</eqn>
				<units>kh/y</units>
			</aux>
			<aux name="Demand for fossil electricity TWh/y">
				<eqn>MAX(0, &quot;Demand_for_electricity_TWh/y&quot;-&quot;Low-carbon_el_production_TWh/y&quot;)</eqn>
				<units>TWh/y</units>
			</aux>
			<aux name="8 khours per year">
				<doc>	8760 hours in a year</doc>
				<eqn>8</eqn>
				<units>kh/y</units>
			</aux>
			<aux name="sFCUTeoLOFC&gt;0">
				<doc>	was 0.1, should be 1?</doc>
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal life of fossil el capacity y">
				<doc>	was 25</doc>
				<eqn>40</eqn>
				<units>y</units>
			</aux>
			<aux name="Low-carbon el production TWh/y">
				<eqn>&quot;Renewable_electricity_production_TWh/y&quot;+&quot;Nuclear_electricity_production_TWh/y&quot;</eqn>
				<units>TWh/y</units>
			</aux>
			<aux name="Life of fossil el capacity y">
				<eqn>Normal_life_of_fossil_el_capacity_y*&quot;FCUTeoLOFC_(1)&quot;</eqn>
				<units>y</units>
			</aux>
			<aux name="FCUTeoLOFC (1)">
				<eqn>1+&quot;sFCUTeoLOFC&gt;0&quot;*((&quot;Fossil_capacity_up-time_kh/y&quot;/&quot;8_khours_per_year&quot;)-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Fossil fuels for electricity Mtoe/y">
				<eqn>&quot;Fossil_electricity_production_TWh/y&quot;/&quot;4_TWh-el_per_Mtoe&quot;</eqn>
				<units>Mtoe/y</units>
			</aux>
			<aux name="Electricity balance (1)">
				<eqn>&quot;Electricity_production_TWh/y&quot;/&quot;Demand_for_electricity_TWh/y&quot;</eqn>
				<units>1</units>
			</aux>
			<flow name="Increase in extra energy productivity index 1/y">
				<eqn>&quot;Extra_energy_productivity_index_2022=1&quot;*0+STEP(&quot;Extra_ROC_in_energy_productivity_after_2022_1/y&quot;, 2022)</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="Extra energy productivity index in 2022 (1)">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Life of renewable el capacity y">
				<eqn>40</eqn>
				<units>y</units>
			</aux>
			<aux name="Nuclear capacity GW">
				<eqn>TIME</eqn>
				<gf>
					<yscale min="75" max="310"/>
					<xpts>1980,2000,2020,2098.9</xpts>
					<ypts>75,310,310,310</ypts>
				</gf>
				<units>GW</units>
			</aux>
			<aux name="Nuclear capacity up-time kh/y">
				<doc>	Full time is 8760 h/y</doc>
				<eqn>8</eqn>
				<units>h/y</units>
			</aux>
			<aux name="Renewable el capacity in 1980 GW">
				<doc>	was 200</doc>
				<eqn>300</eqn>
				<units>GW</units>
			</aux>
			<aux name="Fossil el capacity in 1980 GW">
				<doc>	was 950</doc>
				<eqn>980</eqn>
				<units>GW</units>
			</aux>
			<aux name="Extra ROC in energy productivity after 2022 1/y">
				<doc>	0.002 in TLTL and 0.004 in GL
		220221 changed from 0 to 0.002 to match SSP</doc>
				<eqn>0.002</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Extra cost of Energy Turnaround as share of GDP (1)">
				<eqn>( IF TIME &gt; 2022 THEN (&quot;Cost_of_energy_G$/y&quot;-&quot;Traditional_cost_of_energy_G$/y&quot;)/&quot;GDP_G$/y&quot; ELSE 0 )</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of energy G$/y">
				<eqn>&quot;Traditional_cost_of_electricity_G$/y&quot;+&quot;Traditional_cost_of_fossil_fuel_for_non-el_use_G$/y&quot;+&quot;Traditional_grid_cost_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Ratio of Energy cost to Trad energy cost (1)">
				<eqn>&quot;Cost_of_energy_G$/y&quot;/&quot;Traditional_cost_of_energy_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of electricity $/kWh">
				<doc>	80% fossil at 0.03, 15 % nuclear at 0.033, 5 % hydro at 0.025</doc>
				<eqn>0.03</eqn>
				<units>$/kWh</units>
			</aux>
			<aux name="Traditional cost of electricity G$/y">
				<eqn>(&quot;Demand_for_electricity_before_NE_TWh/y&quot;*&quot;Traditional_cost_of_electricity_$/kWh&quot;/1000)*&quot;Adjustment_factor_to_make_costs_match_1980-2022_(1)&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Adjustment factor to make costs match 1980-2022 (1)">
				<eqn>1.35</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of energy as share of GDP (1)">
				<eqn>&quot;Traditional_cost_of_energy_G$/y&quot;/&quot;GDP_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Traditional cost of fossil fuel for non-el use $/toe">
				<doc>	Cost per ton is cost per barrel times 6.
		33% coal at 30*6=180, 33 % oil at 50*6=300, 33 % gas at 40*6=240</doc>
				<eqn>240</eqn>
				<units>$/toe</units>
			</aux>
			<flow name="Addition of sun and wind capacity GW/y">
				<eqn>&quot;Addition_of_renewable_el_capacity_GW/y&quot;</eqn>
				<units>Gha/Year</units>
			</flow>
			<flow name="Addition of renewable el capacity GW/y">
				<eqn>MAX(0, (Desired_renewable_el_capacity_change_GW/Renewable_el_construction_time_y)+(&quot;Discard_of_renewable_el_capacity_GW/y&quot;))</eqn>
				<units>GW/Year</units>
