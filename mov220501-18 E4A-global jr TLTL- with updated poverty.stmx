	<model name="Public_sector">
		<variables>
			<stock name="TFP excluding effect of 5TAs (1)">
				<eqn>&quot;TFP_in_1980_(1)&quot;</eqn>
				<inflow>&quot;Change_in_TFP_1/y&quot;</inflow>
				<units>1</units>
			</stock>
			<aux name="Infrastructure purchases ratio y">
				<doc>	Is te equivalent of the capital output ratio in the productive economy.
		Happens to be 6 years rather than 2.5</doc>
				<eqn>Capacity_PUS_Gcu/&quot;Govmnt_purchases_G$/y&quot;</eqn>
				<units>y</units>
			</aux>
			<aux name="Reduction in TFP from unprofitable activity (1)">
				<eqn>SMTH1(&quot;Productivity_loss_from_unprofitable_activity_(1)&quot;, Investment_planning_time_y+Construction_time_PIS_y)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="Govmnt spending as share of GDP">
				<eqn>&quot;Govmnt_spending_G$/y&quot;/&quot;GDP_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<flow name="Change in TFP 1/y">
				<eqn>&quot;Rate_of_technological_advance_1/y&quot;*&quot;TFP_excluding_effect_of_5TAs_(1)&quot;</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="sIPReoVPSS&gt;0">
				<doc>	Guess</doc>
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="TFP in 1980 (1)">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Public services per person k$/p/y">
				<eqn>&quot;Value_of_public_services_supplied_G$/y&quot;/Population_Mp</eqn>
				<units>k$/(p*y)</units>
			</aux>
			<aux name="Value of public services supplied G$/y">
				<eqn>&quot;Govmnt_purchases_G$/y&quot;*&quot;Productivity_of_public_purchases_(1)&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Productivity of public purchases (1)">
				<eqn>MAX(0, 1+&quot;sIPReoVPSS&gt;0&quot;*LN(Infrastructure_purchases_ratio_y/Infrastructure_purchases_ratio_in_1980_y))</eqn>
				<units>1</units>
			</aux>
			<aux name="Infrastructure purchases ratio in 1980 y">
				<eqn>1.2</eqn>
				<units>y</units>
			</aux>
			<aux name="Cost of TAs in 2022 G$/y">
				<eqn>9145</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Extra cost of TAs from 2022 G$/y" access="output">
				<eqn>MAX(0, &quot;Cost_of_TAs_G$/y&quot;-&quot;Cost_of_TAs_in_2022_G$/y&quot;)</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Extra cost of TAs as share of GDP (1)">
				<eqn>&quot;Extra_cost_of_TAs_from_2022_G$/y&quot;/&quot;GDP_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Productivity loss from unprofitable activity (1)">
				<eqn>&quot;Extra_cost_of_TAs_as_share_of_GDP_(1)&quot;*&quot;Fraction_unprofitable_activity_in_TAs_(1)&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction unprofitable activity in TAs (1)">
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Indicated TFP (1)" access="output">
				<eqn>&quot;TFP_including_effect_of_5TAs_(1)&quot;*OWeoTFP</eqn>
				<units>1</units>
			</aux>
			<aux name="TFP including effect of 5TAs (1)">
				<doc>	was
		&quot;TFP excluding effect of 5TAs (1)&quot; * ( 1 - &quot;Reduction in TFP from less-cost-effective activities (1)&quot; )</doc>
				<eqn>&quot;TFP_excluding_effect_of_5TAs_(1)&quot;*(1-&quot;Reduction_in_TFP_from_unprofitable_activity_(1)&quot;)</eqn>
				<units>1</units>
			</aux>
			<aux name="OWeoTFP">
				<eqn>( IF TIME &gt; 2022 THEN 1+&quot;sOWeoTFP&lt;0&quot;*(Observed_warming_deg_C/Observed_warming_in_2022_deg_C-1) ELSE 1 )</eqn>
				<units>1</units>
			</aux>
			<aux name="sOWeoTFP&lt;0">
				<doc>	220323 Set to - 0.1 to get TFP reduced by 10 % at +2.5 deg C. (dvs -4% per deg C)</doc>
				<eqn>-0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra TA cost in 2100 (share of GDP)">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra cost of TAs as share of GDP (1)">
				<eqn>&quot;xExtra_TA_cost_in_2022_(share_of_GDP)&quot;+RAMP((&quot;xExtra_TA_cost_in_2100_(share_of_GDP)&quot;-&quot;xExtra_TA_cost_in_2022_(share_of_GDP)&quot;)/78, 2022, 2022+78)</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra TA cost in 2022 (share of GDP)">
				<eqn>0</eqn>
			</aux>
			<aux name="Rate of technological advance 1/y">
				<eqn>(&quot;Domestic_rate_of_technological_advance_1/y&quot;+0)*&quot;Reduction_in_ROTA_from_inequality_1/y&quot;+&quot;Imported_ROTA_1/y&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Domestic rate of technological advance 1/y">
				<eqn>(&quot;Domestic_ROTA_in_1980_1/y&quot;+(( IF TIME &gt; 2022 THEN &quot;Extra_domestic_ROTA_from_2022_1/y&quot; ELSE 0 ))*(1+&quot;sSCeoROTA&gt;0&quot;*(&quot;State_capacity_(fraction_of_GDP)&quot;/&quot;State_capacity_in_1980_(fraction_of_GDP)&quot;-1)))</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Reduction in ROTA from inequality 1/y">
				<eqn>MIN(1, 1+&quot;sIIEeoROTA&lt;0&quot;*(&quot;INEQUALITY_INDEX_(1980=1)&quot;/1-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Imported ROTA 1/y">
				<eqn>( IF TIME &gt; 2022 THEN MAX(0, &quot;Max_imported_ROTA_from_2022_1/y&quot;*(1-1*(&quot;GDP_per_person_k$/p/y&quot;/&quot;GDPpp_of_technology_leader_k$/p/y&quot;-1))) ELSE 0 )</eqn>
				<units>1</units>
			</aux>
			<aux name="Max imported ROTA from 2022 1/y">
				<doc>	0 in TLTL and 0.005 in GL
		was at least 0.02 for China from 1980 to 2010: The technological lead of the West added at least 2 % points to China&apos;s growth.</doc>
				<eqn>0</eqn>
				<units>1/y</units>
			</aux>
			<aux name="GDPpp of technology leader k$/p/y">
				<doc>	I assume countries stop copying at GDPpp=15.000 $/p/y</doc>
				<eqn>15</eqn>
				<units>k$/(p*y)</units>
			</aux>
			<aux name="Domestic ROTA in 1980 1/y">
				<doc>	was 0.01, so 0.04</doc>
				<eqn>0.01</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Extra domestic ROTA from 2022 1/y">
				<doc>	220221 Changed from 0 to 0.003 to match SSP2 4-5 6-doubling of GDP from 2000 to 2100</doc>
				<eqn>0.003</eqn>
				<units>1/y</units>
			</aux>
			<aux name="sSCeoROTA&gt;0">
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="State capacity (fraction of GDP)">
				<eqn>&quot;Value_of_public_services_supplied_G$/y&quot;/&quot;GDP_G$/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="State capacity in 1980 (fraction of GDP)">
				<doc>	was 0.15</doc>
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="sIIEeoROTA&lt;0">
				<eqn>-0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="Public spending per person k$/p/y" access="output">
				<eqn>&quot;Govmnt_spending_G$/y&quot;/Population_Mp</eqn>
				<units>k$/(p*y)</units>
			</aux>
			<aux name="Govmnt purchases G$/y" access="input">
				<units>G$/y</units>
			</aux>
			<aux name="Investment planning time y" access="input">
				<units>y</units>
			</aux>
			<aux name="Construction time PIS y" access="input">
				<units>y</units>
			</aux>
			<stock name="Capacity PUS Gcu" access="input">
				<units>Gcu</units>
			</stock>
			<aux name="Observed warming deg C" access="input">
				<units>deg C</units>
			</aux>
			<aux name="Observed warming in 2022 deg C" access="input">
				<units>deg C</units>
			</aux>
			<aux name="Population Mp" access="input">
				<units>Mp</units>
			</aux>
			<aux name="Cost of TAs G$/y" access="input">
				<units>G$/y</units>
			</aux>
			<aux name="GDP G$/y" access="input">
				<units>G$/y</units>
			</aux>
			<aux name="GDP per person k$/p/y" access="input">
				<units>k$/(p*y)</units>
			</aux>
			<aux name="INEQUALITY INDEX (1980=1)" access="input">
				<units>1</units>
			</aux>
			<aux name="Govmnt spending G$/y" access="input">
				<units>G$/y</units>
			</aux>
		</variables>
