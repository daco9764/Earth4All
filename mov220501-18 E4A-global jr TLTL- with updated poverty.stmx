	<model name="Finance">
		<variables>
			<aux name="Finance sector response time y">
				<doc>	was 0.5</doc>
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="Indicated signal rate 1/y">
				<eqn>&quot;Normal_signal_rate_1/y&quot;*(1+&quot;sINeoSR&gt;0&quot;*(&quot;Perceived_inflation_CB_1/y&quot;/&quot;Inflation_target_1/y&quot;-1)+&quot;sUNeoSR&lt;0&quot;*(&quot;Perceived_unemployment_CB_(1)&quot;/&quot;Unemployment_target_(1)&quot;-1))</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Signal rate adjustment time y">
				<doc>	was 0.3</doc>
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="sUNeoSR&lt;0">
				<doc>	was -0.5, so -1</doc>
				<eqn>-1.5</eqn>
				<units>1/yy</units>
			</aux>
			<aux name="sINeoSR&gt;0">
				<doc>	Should be 1? Was 0, so 0.2</doc>
				<eqn>0.7</eqn>
				<units>1</units>
			</aux>
			<aux name="Unemployment target (1)">
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="Inflation target 1/y">
				<eqn>0.02</eqn>
				<units>1/y</units>
			</aux>
			<aux name="10-yr govmnt interest rate 1/y">
				<eqn>&quot;Govmnt_borrowing_cost_1/y&quot;+&quot;Expected_long_term_inflation_1/y&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Expected long term inflation 1/y">
				<eqn>SMTH1(&quot;Perceived_inflation_CB_1/y&quot;, Inflation_expectation_formation_time_y, &quot;Inflation_in_1980_(1/y)&quot;)</eqn>
				<isee:delay_aux/>
				<units>1/y</units>
			</aux>
			<aux name="Perceived inflation CB 1/y">
				<eqn>SMTH1(&quot;Inflation_rate_1/y&quot;, Inflation_perception_time_CB_y, &quot;Inflation_in_1980_(1/y)&quot;)</eqn>
				<isee:delay_aux/>
				<units>1/y</units>
			</aux>
			<aux name="Inflation expectation formation time y">
				<eqn>10</eqn>
				<units>y</units>
			</aux>
			<aux name="Inflation in 1980 (1/y)">
				<eqn>0.02</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Corporate borrowing cost in 1980 1/y" access="output">
				<doc>	220207 corrected an error!&quot;Normal corporte credit risk&quot; appeared twice in sum.</doc>
				<eqn>&quot;Normal_signal_rate_1/y&quot;+&quot;Normal_basic_bank_margin_1/y&quot;+&quot;Normal_bank_operating_margin_1/y&quot;+&quot;Normal_corporate_credit_risk_1/y&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Normal basic bank margin 1/y">
				<eqn>0.005</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Normal bank operating margin 1/y">
				<eqn>0.015</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Corporate borrowing cost 1/y" access="output">
				<eqn>&quot;Cost_of_capital_for_secured_debt_1/y&quot;+&quot;Normal_corporate_credit_risk_1/y&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Inflation perception time CB y">
				<eqn>1</eqn>
				<units>y</units>
			</aux>
			<aux name="Cost of capital for secured debt 1/y">
				<eqn>SMTH1(&quot;3m_interest_rate_1/y&quot;+&quot;Normal_bank_operating_margin_1/y&quot;, Finance_sector_response_time_y, &quot;3m_interest_rate_1/y&quot;+&quot;Normal_bank_operating_margin_1/y&quot;)</eqn>
				<isee:delay_aux/>
				<units>1/y</units>
			</aux>
			<aux name="Worker borrowing cost 1/y" access="output">
				<eqn>&quot;Cost_of_capital_for_secured_debt_1/y&quot;</eqn>
				<units>1/y</units>
			</aux>
			<aux name="3m interest rate 1/y">
				<eqn>&quot;Central_bank_signal_rate_1/y&quot;+&quot;Normal_basic_bank_margin_1/y&quot;</eqn>
