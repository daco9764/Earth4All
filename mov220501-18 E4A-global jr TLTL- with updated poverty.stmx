	<model name="Other_performance_indicators">
		<variables>
			<aux name="Cost of food and energy TAs G$/y">
				<eqn>&quot;Cost_of_food_G$/y&quot;+&quot;Cost_of_energy_G$/y&quot;</eqn>
			</aux>
			<aux name="Past GDP per person k$/y">
				<eqn>SMTH1(&quot;GDP_per_person_k$/p/y&quot;, Time_to_establish_growth_rate_y, &quot;GDP_per_person_in_1980_k$/p/y&quot;*&quot;Factor_to_avoid_transient_in_growth_rate_(1)&quot;)</eqn>
				<isee:delay_aux/>
				<units>k$/(p*y)</units>
			</aux>
			<aux name="Time to establish growth rate y">
				<eqn>4</eqn>
				<units>y</units>
			</aux>
			<aux name="Factor to avoid transient in growth rate (1)">
				<eqn>0.93</eqn>
				<units>1</units>
			</aux>
			<aux name="RATE OF GROWTH IN GDP PER PERSON 1/y">
				<eqn>((&quot;GDP_per_person_k$/p/y&quot;-&quot;Past_GDP_per_person_k$/y&quot;)/&quot;Past_GDP_per_person_k$/y&quot;)/Time_to_establish_growth_rate_y</eqn>
				<units>1/y</units>
			</aux>
			<aux name="Cost of TAs G$/y" access="output">
				<eqn>&quot;Cost_of_food_and_energy_TAs_G$/y&quot;</eqn>
				<units>G$/y</units>
			</aux>
			<aux name="Cost of food G$/y" access="input">
				<units>G$/y</units>
			</aux>
			<aux name="Cost of energy G$/y" access="input">
				<units>G$/y</units>
			</aux>
			<aux name="GDP per person k$/p/y" access="input">
				<units>k$/(p*y)</units>
			</aux>
			<aux name="GDP per person in 1980 k$/p/y" access="input">
				<units>k$/(p*y)</units>
			</aux>
			<aux name="Inequity effect on logistic k (1) 1">
				<eqn>1+&quot;sINEeoLOK&lt;0_1&quot;*(&quot;Inequality_(1)_2&quot;/0.5-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="sINEeoLOK&lt;0 1">
				<doc>	was - 1</doc>
				<eqn>-0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal k (1) 1">
				<doc>	was 1, so 0.3, so 0.15, so 0.2
		goal is to have 600 Mp &gt; 15k$/y in 2000, ie 10 % of population at the time. And 50 % &gt; 15 k$/p/y in 2020</doc>
				<eqn>0.08</eqn>
				<units>1</units>
			</aux>
			<aux name="SDG1: Population below\n6.85 $/p/day Mp">
				<eqn>Population_Mp_2*&quot;Fraction_below_6.85_$/p/day&quot;</eqn>
				<units>Mp</units>
			</aux>
			<aux name="Logistic k (1) 1">
				<eqn>&quot;Normal_k_(1)_1&quot;*&quot;Inequity_effect_on_logistic_k_(1)_1&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction below\n6.85 $/p/day" access="output" isee:autocreated="true">
				<eqn>1-(1/(1+EXP(-&quot;Logistic_k_(1)_1&quot;*(&quot;GDP_per_person_$/p/day&quot;-(13.9*1000/365.25)))))</eqn>
			</aux>
			<aux name="GDP per person $/p/day">
				<eqn>&quot;GDP_per_person_k$/p/y&quot;*1000/365.25</eqn>
			</aux>
			<aux name="Inequality (1) 2" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>1</units>
			</aux>
			<aux name="Population Mp 2" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
		</variables>
